.model-select-text { height: 0.6rem; font-size: 0.6rem; text-align: center; color: #fff; background: url(../images/indexnew/xjt-icon.png) no-repeat 2.5rem 0; cursor: pointer; -moz-user-select: none; -webkit-user-select: none; user-select: none; }
.model-select-option { display: none; position: absolute; background: #fff; width: 100%; left: -1px; border: 1px solid #89b646; border-top: none; }
.model-select-option li { height: 0.9rem; line-height: 0.9rem; color: #666; font-size: 0.5rem; cursor: pointer; }
.model-select-option li.seleced { background: #89b646; color: #fff; }

.qrCode-box{ display: none; width: 8rem; height: 8rem; position: fixed; z-index: 21; top: 50%; margin-top: -4rem; left: 50%; margin-left: -4rem; }
.qrCode-box img{ width: 100%; height: 100%; }

.hgs-other-recommend .img-list-module a{ display: block; }
.rec-img a,.rec-img img{ display: block; width: 100%; height: 100%; }
.buying-info{ width: 8.5rem; padding-top: 0 !important; }
.hgs-nav,.hgs-nav .swiper-container{ min-height: 5rem;}
